                "dist": {
                    "url": "https://code.highcharts.com/8.1.0/highcharts-3d.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "google/charts",
                "version": "45",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "google_charts"
                },
                "dist": {
                    "url": "https://www.gstatic.com/charts/loader.js",
                    "type": "file"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "jquery/select2",
              "version": "4.0.5",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/select2/select2/archive/4.0.5.zip",
                "type": "zip"
              }
            }
          }
    ],
    "require": {
        "billboardjs/billboard": "1.6.2",
        "c3js/c3": "0.4.18",
        "chartjs/chartjs": "2.7.2",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "d3/d3": "3.5.17",
        "d3v4/d3": "4.9.1",
        "doctrine/annotations": "^1.13",
        "dompdf/dompdf": "0.8.0",
        "drupal/bootstrap_barrio": "~5.1.0",
        "drupal/chartjs_api": "^1.1",
        "drupal/charts": "^5.0@alpha",
        "drupal/config_filter": "^1",
        "drupal/console": "~1.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/devel": "^4.1",
        "drupal/email_verification": "^1.0",
        "drupal/entity_print": "^2.2",
        "drupal/features": "^3.12",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_login": "^1.0",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/optgroup_taxonomy_select": "^1.1",
        "drupal/phpmailer_smtp": "^2.0",
        "drupal/recaptcha": "^3.0",
        "drupal/select2": "^1.13",
        "drupal/term_csv_export_import": "^3.0@alpha",
        "drupal/user_email_verification": "^1.1",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_date_format_sql": "^3.0@alpha",
        "drupal/webform": "^6.1",
        "drupal/webform_config_ignore": "^1.2",
        "drush/drush": "^11.0",
        "google/charts": "45",
        "highcharts/3d": "8.1.0",
        "highcharts/accessibility": "8.1.0",
        "highcharts/export-data": "8.1.0",
        "highcharts/exporting": "8.1.0",
        "highcharts/highcharts": "8.1.0",
        "highcharts/more": "8.1.0",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "tecnickcom/tcpdf": "~6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/console-extend-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "enable-patching": true,
        "patches": {
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "libraries/{$name}": ["type:drupal-library"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "kint-php/kint": "^4.1"
    }
}

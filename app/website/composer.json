            "package": {
                "name": "d3v4/d3",
                "version": "4.9.1",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "d3v4"
                },
                "dist": {
                    "url": "https://cdnjs.cloudflare.com/ajax/libs/d3/4.9.1/d3.js",
